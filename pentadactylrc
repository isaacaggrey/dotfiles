"1.0rc1

group user

map , <Leader>
map -count -modes=i jj -builtin <Esc>
map -count -modes=n ; -builtin <count>:
map -count -modes=n : -builtin <count>;
map -count -modes=n j -builtin <count>4j
map -count -modes=n k -builtin <count>4k
map -count -modes=n <Leader><Space> -builtin <count>:nohlfind<Return>
set defsearch=duckduckgo
set hintkeys=aoeuidhtns
set hlfind
set newtab=help
set titlestring='Mozilla Firefox'

" vim: set ft=pentadactyl:
